{
  "name": "seery-smartcontracts",
  "version": "1.0.0",
  "description": "Seery prediction market smart contracts",
  "scripts": {
    "test": "hardhat test",
    "test:coverage": "hardhat coverage",
    "compile": "hardhat compile",
    "deploy": "hardhat run scripts/deploy.js --network bnbTestnet",
    "deploy:predictions": "hardhat run scripts/deployPredictions.js --network bnbTestnet",
    "deploy:predictions:local": "hardhat run scripts/deployPredictions.js --network localhost",
    "check:address": "hardhat run scripts/checkAddress.js --network bnbTestnet",
    "check:address:local": "hardhat run scripts/checkAddress.js --network localhost",
    "verify:contract": "hardhat run scripts/verifyContract.js --network localhost",
    "check:owner": "hardhat run scripts/checkOwner.js --network bnbTestnet",
    "check:owner:mainnet": "hardhat run scripts/checkOwner.js --network bnbMainnet",
    "withdraw:earnings": "hardhat run scripts/withdrawEarnings.js --network bnbTestnet",
    "withdraw:earnings:mainnet": "hardhat run scripts/withdrawEarnings.js --network bnbMainnet",
    "withdraw:earnings:local": "hardhat run scripts/withdrawEarnings.js --network localhost",
    "node": "hardhat node"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^6.1.1",
    "hardhat": "^2.22.0",
    "chai": "^4.3.10",
    "dotenv": "^16.3.1"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=11.0.0"
  }
}

