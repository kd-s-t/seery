# AWS Configuration
aws_region  = "us-east-1"
environment = "production"

# S3 Bucket Configuration
s3_bucket_name   = "production-seer-coin-images"
enable_versioning = false

# Cost Optimization (AWS Free Tier)
# Lifecycle Policy: Auto-deletes old objects after X days (set to false to keep all objects)
enable_lifecycle_policy    = false
object_expiration_days     = 365

# CORS Configuration
allowed_origins = [
  "https://theseery.com",
  "http://localhost:3015",
  "https://seery-nextjs-hyxh.vercel.app"
]

